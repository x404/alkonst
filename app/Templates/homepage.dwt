<body>

<div id="fullpage">
	<div id="map">
		<div id="ymap"></div>
		<button type="button" data-dismiss="modal" class="close-map" aria-label="Закрыть карту">
			<svg width="28" height="28">
				<line x1="0" y1="0" x2="28" y2="28"/>
				<line x1="28" y1="0" x2="0" y2="28"/>
			</svg>
		</button>
	</div>
	
	<!-- TemplateBeginEditable name="EditRegion3" -->
	<!-- TemplateEndEditable -->

	<!-- =subscribe -->
	<section class="subscribe section pattern-w">
		<div class="container">
			<header>
				<h1>Подписаться на рассылку</h1>
			</header>
			<div class="row">
				<div class="col-lg-8 offset-lg-2">
					<form action="#" method="post">
						<fieldset>
							<div class="d-flex">
								<input type="email" name="email" value="" placeholder="Ваш email" class="form-control" aria-label="Ваш email" >
								<input type="submit" class="submit" value="Подписаться" />
							</div>
						</fieldset>
					</form>				
				</div>
			</div>
		</div>
	</section>
	<!-- =/subscribe -->
	
	<!-- =footer -->
	<footer class="footer text-center">
		<div class="container">
			<a href="/" title="На главную">
				<picture>
					<source srcset="/wp-content/themes/alkonost/images/logo-sm.webp" type="image/webp">
					<img src="/wp-content/themes/alkonost/images/logo-sm.png" alt="" />	
				</picture>
			</a>
			<div class="social d-flex align-center justify-content-center">
